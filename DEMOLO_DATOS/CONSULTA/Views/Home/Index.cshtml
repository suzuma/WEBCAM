
@{
    ViewBag.Title = "CATALOGO DE REGISTROS";
    List<DEMELO_DATOS.Modelo.Empleado> datos = ViewBag.Datos;
}

<div class="container">
    @foreach(DEMELO_DATOS.Modelo.Empleado item in datos){ 
    <div class="col-xs-4">
        <div class="row">
            <img class="card-img-top img-cand img-fluid img-thumbnail" 
                 src="data:image/png;base64,@item.sImagen" alt="@item.sNombre">
        </div>
        <div class="row">
            <ul class="list-group">
                <li class="list-group-item">Folio: @item.EmpleadoId</li>
                <li class="list-group-item">Nombre: @item.sNombre</li>
                <li class="list-group-item">Apellidos: @item.sApellido</li>                         
            </ul>
        </div>
    </div>
    }
</div>

